    "import turtle\n",
    "\n",
    "def spiral():\n",
    "    amy = turtle.Turtle()\n",
    "    turtle.bgcolor(\"black\")\n",
    "    amy.color(\"purple\")\n",
    "    sides = range(100)\n",
    "    for side in sides:\n",
    "        amy.forward(side)\n",
    "        amy.right(20)\n",
    "\n",
    "spiral()\n"
   ]
