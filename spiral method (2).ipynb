   "source": [
    "import turtle\n",
    "\n",
    "def spiral(sides,angle,color,width):\n",
    "    amy = turtle.Turtle()\n",
    "    turtle.bgcolor(\"black\")\n",
    "    amy.color(color)\n",
    "    amy.width(width)\n",
    "    for side in range(sides):\n",
    "        amy.forward(side)\n",
    "        amy.right(angle)\n",
    "spiral(150,-30,\"blue\",10)\n"
   ]
