   "source": [
    "import turtle\n",
    "myshape = turtle.Turtle()\n",
    "myshape.color(\"green\")\n",
    "def zoom(sides, length):\n",
    "    for side in range(sides):\n",
    "        myshape.forward(length)\n",
    "        angle = 360 / sides\n",
    "        myshape.right(angle)\n",
    "\n",
    "for angle in [3,4,5,6,7,8,9,10]:\n",
    "    zoom(angle, 100)\n"
   ]
