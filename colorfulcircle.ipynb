    "jack = turtle.Turtle()\n",
    "jack.color(\"yellow\")\n",
    "\n",
    "for side in range(4):\n",
    "    if side == 1:\n",
    "        jack.color(\"blue\")\n",
    "    if side == 2:\n",
    "        jack.color(\"yellow\")\n",
    "    jack.forward(100)\n",
    "    jack.right(90)\n",
